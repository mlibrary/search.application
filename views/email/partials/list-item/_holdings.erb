<%# If holdings %>
<%# START TOP BORDER %>
<table align="center" border="0" cellpadding="0" cellspacing="0" role="presentation" width="100%">
  <tr>
    <td bgcolor="#e5e9ed" height="1" style="background: #e5e9ed; background-color: #e5e9ed; font-size: 1px; height: 1px; line-height: 1px">&nbsp;</td>
  </tr>
</table>
<%# END TOP BORDER %>

<table align="center" border="0" cellpadding="0" cellspacing="0" role="presentation" width="100%" style="min-width: 100%;">
  <tr>
    <th bgcolor="#f7f8f9" class="holdings-summary" style="background: #f7f8f9; background-color: #f7f8f9; padding: 12px 16px; text-align: left;">
      <%# If holdings.length > 3 %>
      <span style="color: #637381; font-weight: 400">
        There are multiple items, locations or volumes associated with this record; please see the catalog record for full details.
      </span>
      <%# Else %>
      Online Resources
    </th>
  </tr>
  <%# If holdings.length <= 3 %>
  <tr>
    <td class="holdings-table" style="padding: 16px; text-align: left;">
      <table align="center" border="0" cellpadding="0" cellspacing="0" role="presentation" width="100%" style="min-width: 100%;">
        <tr class="holdings-fields">
          <% ["Link", "Description", "Source"].each do |field| %>
            <th class="responsive-cell holding-field" style="border-bottom: 2px solid #e5e9ed; color: #637381; padding: 0px 16px 8px 0px; text-align: left; width: 176px; vertical-align: top; font-weight: 700;">
              <%= field %>
            </th>
          <% end %>
        </tr>
        <tr class="holdings-data">
          <% [
            link_to(body: "Available Online", url: "https://example.com"),
            "Available from 2001.",
            "Miscellaneous Ejournals. Miscellaneous Ejournals. Open access for all users. Authorized U-M users (+ guests in U-M Libraries)"
          ].each do |data| %>
            <td class="responsive-cell holding-data" style="padding: 8px 16px 8px 0px; text-align: left; vertical-align: top;">
              <%= data %>
            </td>
          <% end %>
        </tr>
      </table>
    </td>
  </tr>
</table>