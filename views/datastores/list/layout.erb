<div class="list">
  <%= h1(body: @presenter.page_title) %>
  <p>Items in this list are stored temporarily (within a single session).</p>
  <div class="container__rounded list__empty">
    <p>
      <span class="strong">This list is empty.</span>
      Continue searching to add to this list.
    </p>
  </div>
  <div class="list__actions">
    <%= erb :'datastores/partials/_actions', locals: { patron: @patron } %>
    <div class="list__actions--utilities">
      <%= erb :'datastores/list/partials/_select-all' %>
      <%= erb :'datastores/list/partials/_deselect-all' %>
      <%= erb :'datastores/list/partials/_remove-selected' %>
      <%= erb :'datastores/list/partials/_in-list' %>
    </div>
  </div>
  <%= erb :'datastores/list/partials/_list-item' %>
</div>
