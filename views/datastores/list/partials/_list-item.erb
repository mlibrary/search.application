<li class="container__rounded list__item list__item--clone">
  <div class="list__item--header">
    <%= erb :'datastores/list/partials/list-item/_checkbox' %>
    <h3 class="list__item--title h4">
      <%= link_to(body: "Original Title", url: "datastore/recordId", classes: ["list__item--title-original"]) %>
      <span class="list__item--title-transliterated h5">
        Transliterated Title
      </span>
    </h3>
  </div>
  <table class="metadata">
    <thead class="visually-hidden">
      <tr>
        <th scope="col">Field</th>
        <th scope="col">Data</th>
      </tr>
    </thead>
    <tbody>
      <tr class="metadata__row--clone">
        <th scope="row">
          Field
        </th>
        <td>
          <span class="metadata__data--original">
            Original Data
          </span>
          <span class="metadata__data--transliterated">
            Transliterated Data
          </span>
        </td>
      </tr>
    </tbody>
  </table>
</li>
